   "source": [
    "# Add the whole word frequency dictionary to the dataframe\n",
    "def word_freq3 (index):\n",
    "    \n",
    "    terms = news_data_by_year_df.loc[index,'Terms_min2']\n",
    "    word_counts = {}\n",
    "    \n",
    "    for term in terms:\n",
    "        if term in word_counts:\n",
    "            word_counts[term] += 1\n",
    "        else:\n",
    "            word_counts[term] = 1\n",
    "    return word_counts"
   ]
